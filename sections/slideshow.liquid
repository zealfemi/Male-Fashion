<!-- Hero Section Begin -->
<div id="carouselExampleFade" class="carousel slide carousel-fade">
  <div class="carousel-inner">
    <div class="carousel-item active">
      <img
        src="..."
        class="d-block w-100"
        alt="...">
    </div>
    <div class="carousel-item">
      <img
        src="..."
        class="d-block w-100"
        alt="...">
    </div>
    <div class="carousel-item">
      <img
        src="..."
        class="d-block w-100"
        alt="...">
    </div>
  </div>
  <button
    class="carousel-control-prev"
    type="button"
    data-bs-target="#carouselExampleFade"
    data-bs-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Previous</span>
  </button>
  <button
    class="carousel-control-next"
    type="button"
    data-bs-target="#carouselExampleFade"
    data-bs-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Next</span>
  </button>
</div>



{% schema %}
  {
    "name": "SlideShow",
    "blocks": [
      {
        "name": "Slide",
        "type": "image",
        "settings": [
          {
            "type": "image_picker",
            "id": "slide_image",
            "label": "Slide Image"
          },
          {
            "type": "collection",
            "id": "collection",
            "label": "Collection"
          },
          {
            "type": "text",
            "id": "title",
            "label": "Title",
            "default": "Title"
          },
          {
            "type": "text",
            "id": "description",
            "label": "Description",
            "default": "Description"
          }, {
            "type": "url",
            "id": "btn_url",
            "label": "Button URL"
          }, {
            "type": "text",
            "id": "btn_text",
            "label": "Button Text"
          }, {
            "type": "url",
            "id": "facebook_url",
            "label": "Facebook URL"
          }, {
            "type": "url",
            "id": "twitter_url",
            "label": "Twitter URL"
          }, {
            "type": "url",
            "id": "instagram_url",
            "label": "Instagram URL"
          }
        ]
      }
    ],
    "presets": [
      {
        "name": "Slideshow"
      }
    ]
  }
{% endschema %}